{
  "name": "Proj1",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "test:build:ios": "xcodebuild -workspace ./Proj1.xcodeproj/project.xcworkspace -configuration release -scheme Proj1 -sdk iphonesimulator -derivedDataPath build",
