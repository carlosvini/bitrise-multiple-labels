    "test:run:android": "wdio wdio.conf.android.js"
  },
  "dependencies": {
    "react": "16.9.0",
    "react-native": "0.62.3"
  },
  "resolutions": {
    "@react-native-community/cli-platform-android": "3.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@react-native-community/eslint-config": "^0.0.5",
    "@wdio/appium-service": "^7.7.3",
    "@wdio/cli": "^7.7.4",
    "@wdio/jasmine-framework": "^7.7.3",
    "@wdio/local-runner": "^7.7.4",
    "@wdio/spec-reporter": "^7.7.3",
    "@wdio/sync": "^7.7.4",
    "appium": "^1.21.0",
    "babel-jest": "^24.9.0",
    "eslint": "^6.4.0",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.56.0",
